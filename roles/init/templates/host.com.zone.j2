$ORIGIN host.com.
$TTL 600    ; 10 minutes
@       IN SOA dns.host.com. dnsadmin.host.com. (
               2020100901 ; serial
               10800      ; refresh (3 hours)
               900        ; retry (15 minutes)
               604800     ; expire (1 week)
               86400      ; minimum (1 day)
               )
            NS dns.host.com.
$TTL 60 ; 1 minute
dns              A "{{ node11 }}"
HDSS7-11         A "{{ node11 }}"
HDSS7-12         A "{{ node12 }}"
HDSS7-21         A "{{ node21 }}"
HDSS7-22         A "{{ node22 }}"
HDSS7-200        A "{{ node200 }}"
